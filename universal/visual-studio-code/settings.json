{
    "editor.fontLigatures": true,
    "editor.fontFamily": "Hack NF Semibold, Consolas, 'Courier New', monospace",
    "editor.fontSize": 17,
    "editor.tabSize": 2,
    "prettier.requireConfig": false,
    "editor.codeActionsOnSave": { "source.fixAll": true },
    "editor.defaultFormatter": "remimarsal.prettier-now",
    "editor.formatOnType": true,
    "eslint.alwaysShowStatus": true,
    "htmlhint.enable": true,
    "[html]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true
    },
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true
    },
    "workbench.iconTheme": "material-icon-theme",
    "workbench.productIconTheme": "material-product-icons",
    "emmet.triggerExpansionOnTab": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": true,
    "tabnine.experimentalAutoImports": true,
    "tabnine.receiveBetaChannelUpdates": true,
    "workbench.sideBar.location": "right",
    "editor.minimap.renderCharacters": false,
    "window.title": "${dirty}${activeEditorShort}${separator}${rootPath} ${separator}${appName}",
    "editor.linkedEditing": true,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": true,
    "debug.toolBarLocation": "docked",
    "editor.lineHeight": 22,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "terminal.explorerKind": "external",
    "terminal.external.osxExec": "iTerm.app",
    "workbench.panel.defaultLocation": "right",
    "editor.suggest.showStatusBar": true,
    "workbench.editor.highlightModifiedTabs": true,
    "explorer.sortOrder": "type",
    "editor.wordWrap": "on",
    "workbench.colorTheme": "Gruvbox Material Dark",
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.enabled": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "terminal.external.windowsExec": "wt.exe",
    "files.exclude": {
      "**/.git": false
    },
    "[python]": {
      "editor.defaultFormatter": "ms-python.python"
  },
    "files.autoSave": "onFocusChange",
    "discord.removeRemoteRepository": true,
    "[css]": {
      "editor.defaultFormatter": "aeschli.vscode-css-formatter"
    },
    "gitlens.showWelcomeOnInstall": false,
    "material-icon-theme.showWelcomeMessage": false,
    "[typescriptreact]": {
      "editor.defaultFormatter": "remimarsal.prettier-now"
    },
    "editor.formatOnSave": true,
    "liveServer.settings.donotVerifyTags": true,
    "explorer.confirmDelete": false,
    "remote.SSH.remotePlatform": {
      "ip hehe": "linux"
    }
  }
